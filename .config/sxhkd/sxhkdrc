super + Return
	$TERMINAL

super + p
	rofi -show run

super + i
	rofi -modi "clipboard:greenclip print" -show clipboard

super + {equal,minus,F1}
	volumectl {5%+,5%-,toggle}

super + BackSpace
	flameshot gui

super + control + l
	slock

#super + equal
	 #volnotify up

#super + minus
	 #volnotify down

#super + F1
	#volnotify toggle
